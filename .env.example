# Google API Key for Gemini
# Get your API key from: https://makersuite.google.com/app/apikey
GOOGLE_API_KEY=your_google_api_key_here

# API Key for self-hosted LLM (AMA activity)
AMA_API_KEY=your_llm_api_key_here

# API URL for the frontend to connect to the backend
VITE_API_URL=http://localhost:3000

# CORS origins (comma-separated list for multiple origins)
CORS_ORIGIN=http://localhost:5173

# Base url for images captured from tool output
API_BASE_URL=http://localhost:5173

# Debug Flags (optional, for development)
# Set to 'true' to log system prompts and template processing
DEBUG_PROMPTS=false

# Set to 'true' to log MCP tool calls and responses
DEBUG_MCP=false
